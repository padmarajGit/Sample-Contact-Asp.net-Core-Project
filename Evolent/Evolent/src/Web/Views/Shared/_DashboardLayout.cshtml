<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="content-type" content="text/html; charset=utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewBag.Title</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.css" />
    <link rel="stylesheet" href="~/lib/bootstrap-multiselect/dist/css/bootstrap-multiselect.css" />
    <link rel="stylesheet" href="~/css/site.css" />
    <link rel="shortcut icon" href="~/favicon.ico" />
    <link href="https://fonts.googleapis.com/css?family=Roboto" rel="stylesheet" type="text/css">

    @*Load the scripts*@
    <script src="~/lib/jquery/dist/jquery.js"></script>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.js"></script>
    <script src="~/js/CommonFunctions.js" asp-append-version="true"></script>
    <script src="~/lib/bootstrap-multiselect/dist/js/bootstrap-multiselect.js"></script>
    <script src="~/lib/bootstrap-validator/js/validator.js"></script>
    <script src="~/js/CommonFunctions.js"></script>

</head>

<body class="siteFrontColor ">
    @await Component.InvokeAsync("DashboardHeader")
    <br />
    <hr class="hearderClass" />

    <div id="BaseUrl" data-baseurl="@Url.Content("~/")"></div>

    <div id="container">
        <div class="row" style="height: 648px; overflow-y:auto; ">
            <div class="col-sm-12">
                <div class="col-sm-10">
                    @RenderBody()
                </div>
            </div>
        </div>
        <div class="row">
            <div class="col-sm-12">
                <span class="form-control text-center">Copyright © 2020 All rights reserved.</span>
            </div>
        </div>

    </div>
</body>
</html>
